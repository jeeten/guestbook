{% extends 'base.html.twig' %}
{% block meta %}
    <meta charset="utf-8">
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta content="width=device-width, initial-scale=1, shrink-to-fit=no" name="viewport">
    <meta content="" name="description">
    <meta content="" name="author">
{% endblock %}
{% block title %}
    GuestBook - Dashboard
{% endblock %}
{% block stylesheets %}
    {# {{ encore_entry_link_tags('app') }} #}
    <link href="{{ asset('app.css') }}" rel="stylesheet" type="text/css">
        <link href="{{ asset('assets/vendor/fontawesome-free/css/all.min.css') }}" rel="stylesheet" type="text/css">
    <link href="{{ asset('assets/css/guest-book.min.css') }}" rel="stylesheet">
{% endblock %}
{% block sidebar %}
    {# {{ include('include/sidebar.html.twig') }} #}
{% endblock %}
{% block contenttop %}
    {{ include('include/content_top.html.twig') }}
{% endblock %}
{% block flashmessage %}
    {{ include('include/message.html.twig') }}
{% endblock %}
{% block contentmain %}
    {{ include('include/main.html.twig') }}
{% endblock %}
{% block footer %}
    {{ include('include/footer.html.twig') }}
{% endblock %}
{% block modlebox %}
    {{ include('include/modelbox.html.twig') }}
{% endblock %}
{% block javascripts %}
     {# {{ encore_entry_script_tags('app') }} #}
    <script src="{{ asset('assets/vendor/jquery/jquery.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/vendor/bootstrap/js/bootstrap.bundle.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/vendor/jquery-easing/jquery.easing.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/js/guest-book.min.js') }}" type="text/javascript" type="text/javascript"></script>
{% endblock %}
{% block customjs %}{% endblock %}
